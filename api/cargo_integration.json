            "type":"object",
            "properties":{
               "eventType":{
                  "type":"string",
                  "description":"Event type.",
                  "example":"carregado"
               },
               "eventDescription":{
                  "type":"string",
                  "description":"Event description",
                  "example":"Carregamento carregado"
               },
               "cargoId":{
                  "type":"string",
                  "description":"Cargo ID. This value can be a batch or a child cargo identification.",
                  "example":"12930"
               },
               "createdAt":{
                  "format":"date-time",
                  "type":"string",
                  "description":"Creation date. If this value is not defined, we will define the value at the time of request.",
                  "example":"2022-07-07T00:00:00.000Z"
               },
               "contact":{
                  "description":"Transport step customer or branch-office information.",
                  "allOf":[
                     {
                        "$ref":"#/components/schemas/Contact"
                     }
                  ]
               },
               "location":{
                  "description":"Location infos.",
