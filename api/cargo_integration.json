                  "example":"087654227659876"
               },
               "tomadorCpf":{
                  "type":"string",
                  "description":"CPF do tomador",
                  "example":"08765422765"
               },
               "tomadorMun":{
                  "type":"string",
                  "description":"Cidade do tomador",
                  "example":"Coribe"
               },
               "tomadorUf":{
                  "type":"string",
                  "description":"UF do tomador",
                  "example":"BA"
               }
            },
            "required":[
               "totalValue",
               "nCT",
               "serie",
               "dhEmi",
               "xNome",
               "xFant",
               "proPred",
               "vCarga",
               "emiCnpj",
               "emiIe",
               "tomadorNome",
               "tomadorMun",
               "tomadorUf"
            ]
