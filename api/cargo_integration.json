               "nCT":{
                  "type":"string",
                  "description":"Número do CT-e",
                  "example":"1234387"
               },
               "serie":{
                  "type":"string",
                  "description":"Série",
                  "example":"3"
               },
               "dhEmi":{
                  "type":"string",
                  "description":"Data de emissão",
                  "example":"2023-10-21"
               },
               "xNome":{
                  "type":"string",
                  "description":"Nome da transportadora",
                  "example":"Transporta Barcelo"
               },
               "xFant":{
                  "type":"string",
                  "description":"Nome fantasia de transportadora",
                  "example":"Transporta Barcelo"
               },
               "proPred":{
                  "type":"string",
                  "description":"Produto predominante",
                  "example":"ABRT2 SOJA5"
