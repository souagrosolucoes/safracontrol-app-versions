      },
      "schemas":{
         "DeleteAttachmentResponse":{
            "type":"object",
            "properties":{
               "externalId":{
                  "type":"string",
                  "description":"External file id",
                  "example":"12274"
               },
               "msg":{
                  "type":"string",
                  "description":"Error mensagem",
                  "example":"external id not found"
               }
            },
            "required":[
               "externalId",
               "msg"
            ]
         },
         "Contact":{
            "type":"object",
            "properties":{
               "identificationNumber":{
                  "type":"string",
                  "description":"CNPJ.",
                  "example":"04294897000164"
               }
            }
         },
         "Location":{
            "type":"object",
            "properties":{
               "address":{
                  "type":"string",
                  "description":"Localization address.",
                  "example":"RUA PB08, 1540"
               },
               "city":{
                  "type":"string",
                  "description":"City name",
                  "example":"Goiânia"
               },
               "uf":{
                  "type":"string",
                  "description":"State UF",
                  "example":"go"
               },
               "latitude":{
                  "type":"number",
                  "description":"Latitude",
                  "example":-16.6253386
